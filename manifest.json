{
    "name": "InstagramDownloader",
    "description": "Istagram Media Downloader",
    "author": "DiVouz",
    
    "version": "1.1.1",

    "manifest_version": 3,

    "content_scripts": [
        {
            "matches": ["*://*.instagram.com/*"],
            "css": ["style.css"],
            "js": ["content.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "assets/download.png"
            ],
            "matches": ["*://*.instagram.com/*"]
        }
    ],
    "permissions": []
}
